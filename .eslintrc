{
 "extends": [
  "eslint:recommended"
 ],
 "env": {
  "browser": true,
  "node": true,
  "jasmine": true,
  "es6": true
 },
 "rules": {
  "no-console": 2,
  "no-dupe-keys": 2,
  "no-unexpected-multiline": 2,
  "curly": [
   2,
   "multi-line"
  ],
  "dot-notation": 2,
  "eqeqeq": 2,
  "no-caller": 2,
  "no-case-declarations": 2,
  "no-eval": 2,
  "no-empty": 2,
  "no-extend-native": 2,
  "no-extra-bind": 2,
  "no-fallthrough": 0,
  "no-floating-decimal": 2,
  "no-implicit-coercion": 2,
  "no-implied-eval": 2,
  "no-multi-spaces": 2,
  "no-native-reassign": 2,
  "no-unused-expressions": [
   2,
   {
    "allowShortCircuit": true,
    "allowTernary": true
   }
  ],
  "no-with": 2,
  "strict": [
   2,
   "global"
  ],
  "no-delete-var": 2,
  "no-shadow-restricted-names": 2,
  "no-shadow": 2,
  "no-undef-init": 2,
  "no-use-before-define": [
   2,
   "nofunc"
  ],
  "array-bracket-spacing": 2,
  "block-spacing": 2,
  "brace-style": [
   2,
   "1tbs",
   {
    "allowSingleLine": true
   }
  ],
  "camelcase": 2,
  "comma-spacing": 2,
  "comma-style": 2,
  "eol-last": 2,
  "indent": [
   2,
   2,
   {
    "SwitchCase": 1
   }
  ],
  "key-spacing": 2,
  "no-continue": 2,
  "no-lonely-if": 2,
  "no-mixed-spaces-and-tabs": 2,
  "no-spaced-func": 2,
  "no-trailing-spaces": 2,
  "no-underscore-dangle": 0,
  "object-curly-spacing": [
   2,
   "always"
  ],
  "one-var": [
   2,
   "never"
  ],
  "quote-props": [
   2,
   "as-needed"
  ],
  "quotes": [
   2,
   "single",
   "avoid-escape"
  ],
  "semi-spacing": 2,
  "semi": 2,
  "keyword-spacing": 2,
  "space-before-function-paren": [
   2,
   "never"
  ],
  "space-in-parens": 2,
  "space-infix-ops": 2,
  "space-unary-ops": 2,
  "comma-dangle": [
   2,
   "never"
  ],
  "arrow-spacing": 2,
  "arrow-parens": 2,
  "constructor-super": 2,
  "generator-star-spacing": 2,
  "no-class-assign": 2,
  "no-const-assign": 2,
  "no-dupe-class-members": 2,
  "no-this-before-super": 2,
  "object-shorthand": [
   2,
   "always"
  ],
  "prefer-const": 2,
  "prefer-spread": 2,
  "no-var": 2,
 },
 parserOptions: {
   sourceType: "module"
  }
}
